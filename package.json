{
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  },
  "name": "notion-to-html",
  "description": "This is a Notion API response converter to HTML string",
  "version": "0.1.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "ts-node src/develop.ts",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "npm run test && npm run lint && tsc"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ataztech910/notion-to-html.git"
  },
  "author": "Andrei Tazetdinov",
  "keywords": [
    "Notion API",
    "Notion response converter"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ataztech910/notion-to-html/issues"
  },
  "homepage": "https://github.com/ataztech910/notion-to-html#readme",
  "files": [
    "lib/**/*"
  ]
}
